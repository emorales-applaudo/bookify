services:
  bookify-db:
    container_name: Bookify.Db
    environment:
      POSTGRES_DB: bookify
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Edwin\source\repos\Bookify\.containers\database
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
  bookify.api:
    build:
      context: C:\Users\Edwin\source\repos\Bookify
      dockerfile: Bookify.Api/Dockerfile
